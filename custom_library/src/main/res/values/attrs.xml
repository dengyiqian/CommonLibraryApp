<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleCountDown">
        <attr name="inCircleColor" format="reference|color"/>
        <attr name="outCircleColor" format="reference|color"/>
        <attr name="progressColor" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="ShapeTextView">
        <attr name="pressed_color" format="reference|color" />
        <attr name="disable_color" format="reference|color" />
        <attr name="normal_color" format="reference|color" />

        <attr name="corners_radius" format="dimension" />
        <attr name="top_left_radius" format="dimension" />
        <attr name="top_right_radius" format="dimension" />
        <attr name="bottom_left_radius" format="dimension" />
        <attr name="bottom_right_radius" format="dimension" />

        <attr name="solid_color" format="reference|color" />
        <attr name="stroke_color" format="reference|color" />
        <attr name="stroke_width" format="reference|dimension" />
        <attr name="stroke_dash_gap" format="reference|dimension" />
        <attr name="stroke_dash_width" format="reference|dimension" />
    </declare-styleable>

</resources>